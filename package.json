  "author": "denmaseno <agus@senomas.com>",
  "license": "MIT",
  "scripts": {
    "apollo:generate": "npx graphql-schema-typescript generate-ts src/schema/schema.graphql --output src/schema.d.ts",
    "start": "node 'dist/server.js'",
    "build": "yarn apollo:generate && tsc -p . && ncp src/schema dist/schema",
    "start:dev": "yarn build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^12.7.8",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-resolvers-template": "^0.18.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "^0.0.4",
    "graphql-schema-typescript": "^1.3.1",
    "gulp": "^4.0.2",
    "mocha": "^6.2.0",
    "mocha-typescript": "^1.1.17",
    "nodemon": "^1.19.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/js-yaml": "^3.12.1",
    "apollo-server-express": "^2.9.4",
    "bunyan": "^1.8.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "js-yaml": "^3.13.1",
    "ncp": "^2.0.0"
  }
}
